{
  "name": "rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "REST service starter",
  "main": "src/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/server.ts",
    "start:prod": "cross-env NODE_ENV=production node build/server.js",
    "build": "tsc -p .",
    "lint": "eslint --ext .js,.ts --fix src/",
    "typeorm": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "typeorm-seeding": "ts-node ./node_modules/typeorm-seeding/dist/cli.js --configName src/common/ormconfig.ts",
    "seed:config": "npm run typeorm-seeding config",
    "seed:run": "npm run typeorm-seeding seed"
  },
  "keywords": [
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0 <15"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "docdash": "^1.2.0",
    "dotenv": "9.0.0",
    "express": "4.17.1",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.11.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "swagger-ui-express": "4.1.6",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "uuid": "8.3.2",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.6.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
